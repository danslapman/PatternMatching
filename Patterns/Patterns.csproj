<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net45;netstandard2.0</TargetFrameworks>
    <Version>3.3.0</Version>
    <PackageId>pattern_matching</PackageId>
    <Title>Pattern matching</Title>
    <Authors>LeviySoft</Authors>
    <Description>Pattern matching implementation</Description>
    <PackageLicenseUrl>https://github.com/danslapman/PatternMatching/blob/master/copying.txt</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/danslapman/PatternMatching</PackageProjectUrl>
    <RepositoryUrl>https://github.com/danslapman/PatternMatching</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Copyright>LeviySoft</Copyright>
    <PackageReleaseNotes>ToAction()/ToFunc() return MatcherFunc directly instead of creating wrapping delegate</PackageReleaseNotes>
    <PackageTags>csharp patternmatching patterns</PackageTags>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>

</Project>
